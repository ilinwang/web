./flwa1  ./inputs/spgrid_20_20_3_10000_1000_1000_1.sp >> ./results/spgrid_20_20_3_10000_1000_1000_1.txt
rm -f ./inputs/spgrid_20_20_3_10000_1000_1000_1.sp
